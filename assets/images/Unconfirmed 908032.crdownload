</style>
<script>
    //const alertBox = document.querySelector("#alerBox");
    function displayAlertBox(msg, msgType = "success") {

        const box = document.querySelector('#alerBox') !== null ? document.querySelector('#alerBox') : document.createElement('div');
        box.id = "alerBox"
        let html = `
                    <div  class="alert alert-${msgType} alert-dismissible alert">
                        <a href="#" class="close closebtn" data-dismiss="alert" aria-label="close" onclick="closeAlertBox()">&times;</a>
                         ${msg}.
                    </div>
                `;
        box.innerHTML = html;
        document.querySelector("body").insertAdjacentElement("afterbegin", box)
    }
    function closeAlertBox() {
        if (document.querySelector('#alerBox') === null) return;
        document.querySelector('#alerBox').remove();

    }

    document.addEventListener('keydown', (event) => {
        if (event.key === 'Escape') {
            //if esc key was not pressed in combination with ctrl or alt or shift
            const isNotCombinedKey = !(event.ctrlKey || event.altKey || event.shiftKey);
            if (isNotCombinedKey) {
                if (document.querySelector('#alerBox') === null) return;
                closeAlertBox();
            }
        }
    });
</script>


<script>
    document.addEventListener("DOMContentLoaded", function () {
        try {
            ImageObserver("img.lazyload");
        }
        catch (e) {
            console.log(e);
        }
    })
    function ImageObserver(selector) {
        try {
            const imageObserver = new IntersectionObserver((entries, imgObserver) => {
                entries.forEach((entry) => {
                    if (entry.isIntersecting) {
                        const lazyImage = entry.target
                        if (!lazyImage.hasAttribute("data-src")) {
                            console.log("lazyImage.dataset.data-src", lazyImage.dataset.src)
                            lazyImage.src = lazyImage.dataset.src;
                            lazyImage.classList.remove("loader");
                            lazyImage.onerror = () => {
                                lazyImage.onerror = null;
                                lazyImage.src = "http://gadiyadekho.com//img/noimg.png";
                            };
                            lazyImage.classList.remove("lazyload");
                        }
                    }
                })
            });
            //console.log(typeof selector, selector)
            const arr = typeof selector === 'string' ? document.querySelectorAll(selector) ?? [] : selector;
            if (arr) {
                if (Array.isArray(arr)) {
                    arr?.forEach((v) => {
                        if (v) {
                            console.log("ob", v)
                            imageObserver.observe(v);
                        }
                    })
                }
            }
        }
        catch (e) {
            console.log(e);
        }
    }

</script>


